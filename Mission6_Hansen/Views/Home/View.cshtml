@{
}

@model List<Movie>

<table class="table table-bordered">
    <thead class="fw-bold">
        <tr>
            <td>Category: </td>
            <td>Title: </td>
            <td>Year: </td>
            <td>Director: </td>
            <td>Rating: </td>
            <td>Edited: </td>
            <td>Lent to: </td>
            <td>Notes: </td>
        </tr>

    </thead>

    @foreach (var row in Model)
    {
    <tbody>
        <tr>
            <td>@row.Category</td>
            <td>@row.Title</td>
            <td>@row.Year</td>
            <td>@row.Director</td>
            <td>@row.Rating</td>
            <td>@row.Edit</td>
            <td>@row.Lent</td>
            <td>@row.Notes</td>
        </tr>

    </tbody>
    }
</table>